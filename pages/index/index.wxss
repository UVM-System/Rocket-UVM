.container {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.scan-button {
  border: 1px solid #405f80;
  width: 200rpx;
  height: 80rpx;
  border-radius: 10%;
  text-align: center;
  margin-top: 200rpx;
}

.scan-open {
  font-size: 22rpx;
  color: #405f80;
  line-height: 80rpx;
  font-weight: bold;
}